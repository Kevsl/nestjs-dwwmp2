

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {

id String  @id @default(uuid())
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt

email String @unique
hash String @unique

firstName String?
lastName String?
}

model Bikes {
id Int @id @default(autoincrement())
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt
name String?
price Int?

}